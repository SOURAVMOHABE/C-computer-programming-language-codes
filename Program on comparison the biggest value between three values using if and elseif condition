#include<stdio.h>
int main (){
	int a, b, c;
	printf("Enter the value of a \n");
	scanf("%d",&a);
	printf("Enter the value of b \n");
	scanf("%d",&b);
	printf("Enter the value of c \n");
	scanf("%d",&c);
	if(a>b && a>c){
		printf("Biggest value is a");
	}
	else if(b>a && b>c){
		printf("Biggest value is b");
	}
	else{
		printf("Biggest value is c");
	}
	return 0;
}
